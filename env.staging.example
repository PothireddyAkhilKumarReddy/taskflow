# Staging Environment Variables
# Copy this file to .env.staging and update the values

# Database Configuration
DB_PASSWORD=staging-password
DB_HOST=postgres
DB_PORT=5432
DB_NAME=taskflow_staging
DB_USER=postgres

# JWT Configuration
JWT_SECRET=staging-jwt-secret-key

# pgAdmin Configuration
PGADMIN_EMAIL=admin@staging.yourcompany.com
PGADMIN_PASSWORD=staging-pgadmin-password

# Server Configuration
NODE_ENV=staging
PORT=5000

# CORS Configuration
CORS_ORIGIN=http://staging.yourdomain.com
